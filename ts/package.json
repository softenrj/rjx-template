  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx src/server.ts",
    "lint:watch": "onchange \"src/**/*.ts\" -- eslint --fix {{changed}}",
    "dev:hot": "concurrently -n LINT,SERVER --prefix-colors=cyan,green \"npm run lint:watch\" \"tsx watch src/server.ts\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "npx eslint .",
    "prepare": "husky",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "rimraf node_modules coverage dist package-lock.json"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,json,css,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
